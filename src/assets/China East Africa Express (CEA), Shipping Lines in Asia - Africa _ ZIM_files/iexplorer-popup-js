function download_chrome(){window.open("https://www.google.com/chrome/")}function download_edge(){window.open("https://www.microsoft.com/edge")}function close_ie_popup(){PopupElement.style.display="none"}const isIE=!!window.navigator.userAgent.match(/MSIE|Trident/),PopupElement=document.querySelector(".init-ie-popup-wrapper");PopupElement.style.display="none";!sessionStorage.getItem("ie-browser")&&isIE&&(PopupElement.style.display="block",sessionStorage.setItem("ie-browser",!0))